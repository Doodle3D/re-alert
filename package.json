{
  "name": "re-alert",
  "version": "0.2.3",
  "description": "A light-weight web page notification system for React and Redux",
  "keywords": [
    "react",
    "redux",
    "alert",
    "alerts",
    "notification",
    "notifications"
  ],
  "author": "Yang Liu <hi@zesik.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zesik/re-alert"
  },
  "bugs": {
    "url": "https://github.com/zesik/re-alert/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "coveralls": "^2.11.12",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "expect": "^1.20.2",
    "ignore-styles": "^4.0.0",
    "isparta": "^4.0.0",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.1.4",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-addons-css-transition-group": "^0.14.0 || ^15.0.0",
    "react-redux": "^3.0.0 || ^4.0.0",
    "redux": "^2.0.0 || ^3.0.0",
    "redux-thunk": "^2.0.0"
  },
  "scripts": {
    "prepublish": "webpack --progress -p",
    "build": "webpack --progress",
    "test": "mocha --require ignore-styles --compilers js:babel-register test/*.spec.js*",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --include 'src/**/*.js*' node_modules/mocha/bin/_mocha -- --require ignore-styles --compilers js:babel-register --reporter spec test/*.spec.js*",
    "travis-coverage": "babel-node node_modules/isparta/bin/isparta cover --include 'src/**/*.js*' --report lcovonly node_modules/mocha/bin/_mocha -- --require ignore-styles --compilers js:babel-register --reporter spec test/*.spec.js* && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && rm -rf coverage"
  }
}
